# Aliases
alias gits="git status"
alias ls="ls --color=auto -h"
alias la="ls -A"
alias ll="ls -l"
alias lal="ls -Al"
alias l="ll"
alias c="clear"
alias g="git"
alias ssx="DISPLAY=:0.0 ssh -CY"
alias amend="git add . && git commit --amend --no-edit"

arhar () {
    echo "dotfiles loaded";
}

ccc () {
    gcc $1 && ./a.out;
}
